	<ItemGroup>
		<ProjectReference Include="..\ECommons\ECommons\ECommons.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="GatheringPoints\ARR\" />
	  <Folder Include="GatheringPoints\Heavensword\" />
	  <Folder Include="GatheringPoints\Endwalker\" />
	  <Folder Include="GatheringPoints\Dawntrail\" />
	  <Folder Include="GatheringPoints\Shadowbringers\" />
	  <Folder Include="GatheringPoints\Stormblood\" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="9.0.7" />
	  <PackageReference Include="SQLite" Version="3.13.0" />
	  <PackageReference Include="SQLitePCLRaw.bundle_e_sqlite3" Version="2.1.11" />
	  <PackageReference Include="YamlDotNet" Version="16.3.0" />
	</ItemGroup>


</Project>
